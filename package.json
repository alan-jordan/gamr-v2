{
  "name": "gamr",
  "version": "0.2.0",
  "description": "Second attempt at building gamr",
  "repository": "https://github.com/alan-jordan/gamr-v2",
  "main": "server/index.js",
  "scripts": {
    "knex": "knex",
    "dev": "nodemon --watch server server/index & webpack --watch",
    "start": "node server/index",
    "snazzy": "standard | snazzy & ava  -v 'tests/**/*.test.js'",
    "test": "nyc --reporter=html --reporter=text ava -v 'tests/**/*.test.js'",
    "test-coverage": "nyc ava -v 'tests/**/*.test.js'",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "watch": "nodemon -e js,jsx -x npm test",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "lint": "eslint server client test"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "enzyme": "^2.8.2",
    "eslint": "^4.1.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-node": "^1.6.0",
    "eslint-config-promise": "^1.1.3",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^10.2.1",
    "jazzy": "^0.3.18",
    "jsx-loader": "^0.13.2",
    "nock": "^9.0.13",
    "node-mocks-http": "^1.6.4",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.3.8",
    "supertest": "^3.0.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "fs": "0.0.1-security",
    "jsdom": "^11.0.0",
    "jsonwebtoken": "^7.4.1",
    "knex": "^0.13.0",
    "moment": "^2.18.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.2.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sodium": "^2.0.1",
    "sqlite3": "^3.1.8",
    "standard": "^10.0.2",
    "superagent": "^2.2.0"
  }
}
